<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CampRegirsterController</servlet-name>
        <servlet-class>vn.com.iu.scse.sample.sampleservlet.CampRegirsterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberListController</servlet-name>
        <servlet-class>vn.com.iu.scse.sample.sampleservlet.MemberListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CampRegirsterController</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberListController</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
